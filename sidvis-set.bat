:: DIRECTORY PATHS
set sidplayfp_dir=sidvis-sidplayfp
set hvsc_dir=
set ffmpeg_dir=
set wav_dir=

:: TRACK
set sid_file_path=
set track_number=1

:: TIMING
set record_mm_ss=00:11
set fadeout_seconds=10
set add_hvsc_time=1

set fadein_samples=2048
set start_delay_cycles=4096

:: EMULATION
set clock=auto
set sid_model=auto
set combined_waves=average
set filter_curve_6581=1.0
set filter_range_6581=0.5

:: MASTER AUDIO
set ma_record=1
set ma_pan=mono

:: ON-SCREEN WAVEFORMS
set os_record=1
set os_d418_digi=0

:: EXTERNAL TRIGGERS
set xt_record=1
set xt_triggerwaves=1
set xt_no_filter=1

:: DEBUG
set quiet=3
set del_ffmpeg_files=1