:: DIRECTORY PATHS
set sidplayfp_dir=sidvis-sidplayfp
set hvsc_dir=
set ffmpeg_dir=
set wav_dir=

:: TRACK
set sid_file_path=
set track_number=1

:: TIMING
set add_hvsc_time=0
set record_mm_ss=00:11
set fadeout_seconds=10

set fadein_samples=2048
set start_delay_cycles=4096

:: EMULATION
set clock=auto
set sid_model=auto
set filter_curve_6581=1.0
set filter_range_6581=0.5
set combined_waves=average

:: MASTER AUDIO
set MA_enable=0
set pan=m

:: ON-SCREEN WAVEFORMS
set OS_enable=0
set D418_digi=0

:: EXTERNAL TRIGGERS
set XT_enable=0
set FIltered-RegularWaves_Channels=0
set FIltered-TriggerWaves_Channels=0
set NoFilter-RegularWaves_Channels=0
set NoFilter-TriggerWaves_Channels=0